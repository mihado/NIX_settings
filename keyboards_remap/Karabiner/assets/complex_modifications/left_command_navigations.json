{
  "title": "left_command navigations",
  "rules": [
    {
      "description": "left_control+left_cmd+left -> control+left",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "left_control",
                "left_command"
              ]
            }
          },
          "to": [{
            "key_code": "right_arrow",
            "modifiers": [
              "left_control"
            ]
          }]
        }
      ]
    },
    {
      "description": "left_cmd+hjkl -> arrow keys",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [{
            "key_code": "left_arrow"
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [{
            "key_code": "down_arrow"
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [{
            "key_code": "up_arrow"
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [{
            "key_code": "right_arrow"
          }]
        }
      ]
    },
    {
      "description": "left_cmd+nm -> right_cmd+left/right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [{
            "key_code": "left_arrow",
            "modifiers": [
              "right_command"
            ]
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [{
            "key_code": "right_arrow",
            "modifiers": [
              "right_command"
            ]
          }]
        }
      ]
    },
    {
      "description": "left_cmd+uo -> page_up/page_down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [{
            "key_code": "page_up"
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [{
            "key_code": "page_down"
          }]
        }
      ]
    },
    {
      "description": "left_cmd+shift+uo -> cmd+shift+[]",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_shift"
              ]
            }
          },
          "to": [{
            "key_code": "open_bracket",
            "modifiers": [
              "right_command",
              "right_shift"
            ]
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_shift"
              ]
            }
          },
          "to": [{
            "key_code": "close_bracket",
            "modifiers": [
              "right_command",
              "right_shift"
            ]
          }]
        }
      ]
    }
  ]
}
