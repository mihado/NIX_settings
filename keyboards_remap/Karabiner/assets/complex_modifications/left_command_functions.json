{
  "title": "left_command functions",
  "rules": [
    {
      "description": "left_cmd+d delete forward",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [{
            "key_code": "delete_forward"
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_shift"
              ]
            }
          },
          "to": [{
            "key_code": "d",
            "modifiers": [
              "right_command"
            ]
          }]
        }
      ]
    }
  ]
}
